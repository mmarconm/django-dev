{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Django",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/manage.py",
      "args": ["runserver", "0.0.0.0:8000"],
      "django": true,
      "justMyCode": true
    },
    {
      "name": "Gunicor",
      "type": "debugpy",
      "request": "launch",
      "program": "/usr/local/bin/gunicorn",
      "args": [
        "root.wsgi:application",
        "--bind=0.0.0.0:8000",
        "--reload"
      ],
      "django": true,
      "justMyCode": true
    },
    {
      "name": "Extension",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": ["--extensionDevelopmentPath=${workspaceFolder}"]
    }
  ]
}
